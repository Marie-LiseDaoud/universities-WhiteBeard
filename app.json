{
  "expo": {
    "name": "universities-marielise-daoud",
    "slug": "universities-marielise-daoud",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.universities"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.universities"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "universitiesmarelisedaoud",
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-font"
    ]
  }
}
